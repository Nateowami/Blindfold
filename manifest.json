{
  "name": "Blindfold",
  "version": "0.1.0",
  "description": "Gmail extension to remind you when you forget to use BCC",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "lib/jquery-3.1.1.min.js",
    "lib/gmail.js",
    "blindfold.js"
  ],
  "manifest_version": 2
}
